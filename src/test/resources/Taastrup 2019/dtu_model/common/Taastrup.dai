;;; SÆDSKIFTE opvarmningsperiode TAASTRUP inden forsøgsstart efterår 2009 og frem til efteråret 2014 og forsøgstart
;; Spring Barley m udlæg setup.
(defaction "SBarley_management2010" activity
  (wait_mm_dd 11 25)
  (myplowing)
  (wait_mm_dd 4 18)  
  (fertilize (NPK01 (weight 100 [kg N/ha])))
  (seed_bed_preparation)
  (sow "Spring Barley2")
  (sow "Froegraes")
  (wait (or (crop_ds_after "Spring Barley2" 2.0) ;Ripe
            (mm_dd 9 1))) 
  (harvest "Spring Barley2"(stub 10[cm])(stem 0.0[])(leaf 0.0 [])))

;; Red fescue setup.
